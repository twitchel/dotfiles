"$schema" = 'https://starship.rs/config-schema.json'

format = """
[](color_grey)\
$os\
$username\
$hostname\
[](bg:color_blue fg:color_grey)\
$directory\
[](fg:color_blue bg:color_aqua)\
$git_branch\
$git_status\
[](fg:color_aqua bg:color_bg2)\
$fill\
[](fg:color_purple bg:color_bg2)\
$c\
$rust\
$golang\
$nodejs\
$php\
$java\
$kotlin\
$haskell\
$python\
[](fg:color_bg1 bg:color_purple)\
$time\
[ ](fg:color_bg1)\
$line_break$character"""

